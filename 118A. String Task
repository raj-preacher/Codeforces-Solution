#include <bits/stdc++.h>
using namespace std;
int main(){
string str;
string str2="";
cin>>str;
transform(str.begin(), str.end(), str.begin(), ::tolower);
//cout<<str;
for(int i=0;i<str.length();i++){
      if(str[i]=='a'||str[i]=='o'||str[i]=='y'||str[i]=='e'||str[i]=='u'||str[i]=='i'||
      str[i]=='A'||str[i]=='O'||str[i]=='Y'||str[i]=='E'||str[i]=='U'||str[i]=='I'){
            continue;
      }else{
          str2+=".";
          str2+=str[i];
      }
}
cout<<str2;
 return 0;
}
